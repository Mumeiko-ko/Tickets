{
  "manifest_version": 3,
  "name": "無名子大王 (學習用)",
  "version": "0.1",
  "description": "一個協助自動化點擊流程的學習專案",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"  
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [ 
    {
      "matches": ["https://tixcraft.com/*"], 
      "js": ["content.js"],
      "run_at": "document_idle" 
    }
  ]
}